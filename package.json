{
    "name": "oba",
    "version": "1.0.0",
    "description": "oba app server side",
    "main": "index.js",
    "scripts": {
        "start": "node app.js",
        "build:js": "browserify src/js/index.js | uglifyjs -c > public/bundle.js",
        "build:scss-compile": "node-sass  src/scss/ -o src/css/",
        "build:css": "postcss src/css/index.css > public/bundle.css",
        "build": "npm run build:js && npm run build:scss-compile && npm run build:css"
    },
    "author": "Yoeri Pasmans",
    "license": "ISC",
    "dependencies": {
        "global": "^4.3.2"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "body-parser": "^1.17.1",
        "browserify": "^16.1.1",
        "compression": "^1.7.2",
        "cookie-parser": "^1.4.3",
        "cssnano": "^3.10.0",
        "ejs": "^2.5.7",
        "express": "^4.15.2",
        "http": "0.0.0",
        "imagesloaded": "^4.1.4",
        "mapbox": "^1.0.0-beta9",
        "mapbox-gl": "^0.44.1",
        "masonry-layout": "^4.2.1",
        "multer": "^1.3.0",
        "node-fetch": "^2.1.1",
        "node-sass": "^4.7.2",
        "postcss-cli": "^5.0.0",
        "request": "^2.81.0",
        "scssify": "^3.0.1",
        "terraformer-wkt-parser": "^1.1.2",
        "uglify-js-es6": "^2.8.9",
        "window": "^4.2.5"
    },
    "postcss": {
        "plugins": {
            "cssnano": {
                "preset": "default"
            }
        }
    }
}